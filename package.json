    "jwt-decode": "^2.2.0",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-calendar-picker": "^7.0.3",
    "react-native-config": "^0.12.0",
