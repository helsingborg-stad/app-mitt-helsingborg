    "react-native-swiper": "^1.6.0-nightly.5",
    "react-native-tab-view": "^2.15.1",
    "react-native-vector-icons": "^6.6.0",
    "react-native-webview": "^9.4.0",
    "react-native-youtube-iframe": "^1.4.0",
    "styled-components": "^5.0.1",
    "validator": "^13.1.17",
